{
  "name": "modern-saas",
  "author": "Trey M",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "16.6.0",
    "yarn": "1.22.11"
  },
  "scripts": {
    "dev": "nodemon --inspect --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec 'node --inspect=0.0.0.0:9229 --require ts-node/register --require tsconfig-paths/register src/server.ts'",
    "start": "next build && yarn generate && ts-node --transpile-only -r tsconfig-paths/register src/server.ts",
    "generate": "next build && yarn -s run prisma:generate && yarn -s run nexus:generate",
    "nexus:generate": "ts-node --transpile-only src/plugins/graphql/schema",
    "prisma:generate": "prisma generate --schema=./src/plugins/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./src/plugins/prisma/schema.prisma",
    "prisma:deploy": "prisma migrate deploy --schema=./src/plugins/prisma/schema.prisma",
    "prisma:reset": "prisma migrate reset --schema=./src/plugins/prisma/schema.prisma",
    "prisma:pull": "prisma db pull --schema=./src/plugins/prisma/schema.prisma",
    "prisma:introspect": "prisma introspect --schema=./src/plugins/prisma/schema.prisma",
    "prisma:format": "prisma format --schema=./src/plugins/prisma/schema.prisma"
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "@geist-ui/react": "^2.2.0",
    "@geist-ui/react-icons": "^1.0.1",
    "@next-auth/prisma-adapter": "^0.5.4",
    "@prisma/client": "^2.28.0",
    "altair-fastify-plugin": "^4.0.9",
    "axios": "^0.21.1",
    "date-fns": "^2.23.0",
    "dotenv": "^10.0.0",
    "fastify": "^3.19.1",
    "fastify-cors": "^6.0.2",
    "fastify-nextjs": "^6.0.0",
    "fastify-plugin": "^3.0.0",
    "fastify-sensible": "^3.1.1",
    "fastify-static": "^4.2.3",
    "fastify-swagger": "^4.8.4",
    "graphql": "^15.5.3",
    "graphql-scalars": "^1.10.1",
    "lodash": "^4.17.21",
    "mercurius": "^8.2.1",
    "next": "^11.0.1",
    "next-auth": "^3.29.0",
    "next-pwa": "^5.3.1",
    "nexus": "^1.1.0",
    "nexus-prisma": "^0.34.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "swr": "^0.5.6",
    "tailwindcss": "^2.2.7"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.3.1",
    "@types/react": "^17.0.14",
    "autoprefixer": "^10.3.1",
    "fastify-tsconfig": "^1.0.1",
    "nodemon": "^2.0.12",
    "openapi-types": "^9.1.0",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "prisma": "^2.28.0",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.3.5"
  }
}
