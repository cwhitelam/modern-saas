{
  "name": "modern-saas",
  "author": "Trey M",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --inspect --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec 'node --inspect=0.0.0.0:9229 --require ts-node/register --require tsconfig-paths/register src/server.ts'",
    "start": "next build && prisma:generate && ts-node src/server.ts",
    "prisma:generate": "prisma generate --schema=./src/plugins/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./src/plugins/prisma/schema.prisma",
    "prisma:deploy": "prisma migrate deploy --schema=./src/plugins/prisma/schema.prisma",
    "prisma:reset": "prisma migrate reset --schema=./src/plugins/prisma/schema.prisma"
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^2.28.0",
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "fastify": "^3.19.1",
    "fastify-nextjs": "^6.0.0",
    "fastify-plugin": "^3.0.0",
    "next": "^11.0.1",
    "nextjs": "^0.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "swr": "^0.5.6",
    "tailwindcss": "^2.2.7"
  },
  "devDependencies": {
    "@types/node": "^16.3.1",
    "@types/react": "^17.0.14",
    "autoprefixer": "^10.3.1",
    "fastify-tsconfig": "^1.0.1",
    "nodemon": "^2.0.12",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "prisma": "^2.28.0",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": "16.3.0",
    "yarn": "1.22.10"
  }
}
