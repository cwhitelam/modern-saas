generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  uuid  String  @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  email String  @unique
  name  String?
}
